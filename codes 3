dictionary = {"apple": 2, "banana": 4, "orange": 1, "kiwi": 3}
for fruit, count in dictionary.items():
    print("I have {} {}s".format(count, fruit))
for number in range(2, 11, 2): #2, 4, 6, 8, 10
    print(number)
names = ["Alice", "Bob", "Charlie", "Dave"]
for index, name in enumerate(names):
    print("Index {} belongs to {}".format(index, name))
