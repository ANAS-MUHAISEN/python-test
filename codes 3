for each in range(1, 11):
    print(each)
for each in "karabuk istanbul":
    print(each)
liste = [1, 4, 6, 8, 3, 5, 9, 2, 43, 57]
print(sum(liste))
liste1 = [1, 2, 3, 4, 5, 6, -500, 23, 451, 67, 21, 23]
print(min(liste1))
minimum = 100000
for each in liste1:
    if each < minimum:
        minimum = each
    else:
        continue
print(minimum)
